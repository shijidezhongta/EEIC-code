}

QLineEdit {
    background-color: transparent;  /* 背景透明，保持深蓝色背景可见 */
    color: white;                   /* 字体颜色为白色 */
    border: 2px solid #fff7d6;      /* 设置浅蓝色边框，增加视觉效果 */
    border-radius: 8px;             /* 边框圆角效果，增加现代感 */
    padding: 5px;                   /* 内边距，避免文字贴边 */
    font-size: 14px;                /* 设置适当的字体大小 */
}

QLineEdit:focus {
    border-color: #FF4081;          /* 当控件获得焦点时，设置边框颜色为粉红色 */
    background-color: rgba(0, 0, 0, 0.1); /* 设置获得焦点时的背景为透明黑色，突出显示 */
}

QPushButton {
    background-color: #1D2B3A;      /* 深蓝色背景 */
    color: white;                    /* 白色文字 */
    border: 2px solid rgba(255, 255, 255, 0.5);  /* 半透明白色边框，边框变细 */
    border-radius: 10px;             /* 圆角效果 */
    padding: 10px 20px;              /* 内边距 */
    font-size: 16px;                 /* 字体大小 */
    font-weight: bold;               /* 加粗字体 */

}

QPushButton:hover {
    background-color: #4A5B73;       /* 悬停时背景变浅 */
    border-color: rgba(255, 255, 255, 0.7);  /* 悬停时边框颜色稍微变深 */
}

QPushButton:pressed {
    background-color: #2C3E50;       /* 按钮按下时背景变为深色 */
    border-color: rgba(255, 255, 255, 0.7);  /* 按下时边框颜色 */
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);  /* 按下时添加阴影效果 */
}</string>
